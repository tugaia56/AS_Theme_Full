<?xml version="1.0" encoding="utf-8"?>
<selector android:enterFadeDuration="@*android:integer/config_shortAnimTime" android:exitFadeDuration="@*android:integer/config_shortAnimTime"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/ripple_material_dark" />
            <stroke android:width="1.0dip" android:color="@*android:color/accent_material_dark" />
