<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <ripple android:color="@*android:color/transparent">
            <item>
                <shape android:shape="oval">
                    <solid android:color="@*android:color/transparent" />
					<stroke android:width="2.0dip" android:color="@*android:color/accent_material_dark"/>
                </shape>
            </item>
        </ripple>
    </item>
    <item>
        <ripple android:color="@*android:color/ripple_material_dark">
            <item>
                <shape android:shape="oval">
                    <solid android:color="@*android:color/accent_material_dark" />
                </shape>
            </item>
        </ripple>
    </item>
</selector>