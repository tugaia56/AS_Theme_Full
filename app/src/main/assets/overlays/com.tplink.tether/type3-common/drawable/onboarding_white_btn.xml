        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@*android:color/background_dark" />
            <corners android:topLeftRadius="100.0dip" android:topRightRadius="100.0dip" android:bottomLeftRadius="100.0dip" android:bottomRightRadius="100.0dip" />
            <stroke android:width="1.0dip" android:color="@*android:color/accent_material_dark" />
        </shape>
    </item>
