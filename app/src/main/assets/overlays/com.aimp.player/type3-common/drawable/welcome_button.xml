<?xml version="1.0" encoding="utf-8"?>
<selector android:enterFadeDuration="75" android:exitFadeDuration="250"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@*android:color/ripple_material_dark" />
            <corners android:radius="24.0dip" />
            <stroke android:width="1.5dip" android:color="@*android:color/accent_material_dark" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@*android:color/background_dark" />
            <corners android:radius="24.0dip" />
            <stroke android:width="1.5dip" android:color="@*android:color/accent_material_dark" />
        </shape>
    </item>
</selector>